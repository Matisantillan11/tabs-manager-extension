{
  "manifest_version": 3,
  "name": "Tabs Manager",
  "version": "1.0",
  "description": "Manage your browser tabs efficiently.",
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "vite.svg"
  },
  "host_permissions": ["https://developer.chrome.com/*"],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
